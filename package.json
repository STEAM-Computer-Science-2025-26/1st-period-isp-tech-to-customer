{
  "name": "tech-to-customer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "node -e \"const {execSync}=require('child_process');try{execSync('npx prettier . --check',{stdio:'inherit'});}catch(e){execSync('npx prettier . --write',{stdio:'inherit'});}execSync('next build',{stdio:'inherit'});\"",
    "build:cf": "next-on-pages && node ./scripts/cf-pages-output.mjs",
    "start": "next start",
    "preview:cf": "wrangler pages dev .dist",
    "deploy:cf": "wrangler pages deploy .dist",
    "format": "npx prettier . --write",
    "lint": "eslint . -f ./eslint.formatter.js && npx prettier . --check",
    "lint:auto-format": "eslint . -f ./eslint.formatter.js --fix && node -e \"const {execSync}=require('child_process');try{execSync('npx prettier . --check',{stdio:'inherit'});}catch(e){execSync('npx prettier . --write',{stdio:'inherit'});}\"",
    "dev:backend": "tsx services/server.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/jwt": "^10.0.0",
    "@googlemaps/js-api-loader": "^2.0.2",
    "@neondatabase/serverless": "^1.0.2",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "fastify": "^5.7.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "next": "15.5.9",
    "pg": "^8.17.2",
    "prettier": "^3.7.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "regression": "^2.0.1",
    "undici": "^7.19.2",
    "ws": "^8.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.6",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.30",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/regression": "^2.0.6",
    "@types/ws": "^8.18.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "wrangler": "^3.112.0"
  }
}
